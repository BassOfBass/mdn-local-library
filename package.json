{
  "name": "mdn-express-server",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "bin/www.js",
  "engines": {
    "node": "12.18.1"
  },
  "scripts": {
    "start": "node ./bin/www.js",
    "sass": "sass --style=expanded --watch frontend/src/css:public/css",
    "watch": "SET DEBUG=mdn-express-server:* && nodemon -r dotenv/config",
    "dev": "npm-run-all --parallel watch sass"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "http-errors": "~1.6.3",
    "moment": "^2.28.0",
    "mongoose": "^5.10.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.29.0"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "public",
      "frontend"
    ]
  }
}
